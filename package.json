    "@types/json-bigint": "^1.0.1",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^13.7.0",
    "@types/node-cron": "^3.0.1",
    "@types/papaparse": "^5.3.1",
    "@types/supertest": "^2.0.8",
    "chai": "^4.3.6",
