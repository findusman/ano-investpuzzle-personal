{
  "name": "wafflestock-api",
  "version": "1.0.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "nodemon -r tsconfig-paths/register -r ts-node -r ./src/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "prod": "ts-node ./build/server.js",
    "lint": "eslint .",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "test-local": "DOTENV_CONFIG_PATH=.env.local.test mocha --timeout 30000 --exit -r tsconfig-paths/register -r dotenv/config -r ts-node/register ./src/test/**/*.spec.ts",
    "test-local-coverage": "nyc --reporter=html --reporter=text npm run test-local",
    "test": "mocha --timeout 30000 --exit -r tsconfig-paths/register -r dotenv/config -r ts-node/register ./build/test/**/*.spec.js"
  },
  "description": "",
  "dependencies": {
    "@socket.io/redis-adapter": "^7.1.0",
