    "aws-sdk": "^2.1231.0",
    "axios": "^0.23.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.1",
    "bull": "^4.10.1",
    "bull-arena": "^3.30.0",
    "bunyan": "^1.8.15",
    "bunyan-loggly": "^1.4.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "concurrently": "^8.0.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.6.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.1",
    "firebase-scrypt": "^2.2.0",
    "fs": "^0.0.1-security",
    "google-auth-library": "^7.11.0",
    "googleapis": "^108.0.0",
    "handlebars": "^4.7.7",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
